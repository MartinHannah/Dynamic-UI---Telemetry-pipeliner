{
    "parser": "babel-eslint",
    "extends": [
        "airbnb",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "plugins": [
        "flowtype"
    ],
    "env": {
            "browser": true,
            "node": true
        },
    "rules": {
        "react/no-access-state-in-setstate": 0,
        "no-console": 0
    }
}